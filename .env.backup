# Development Environment Configuration
VITE_APP_TITLE=TipTap Collaborative Editor
VITE_WEBSOCKET_URL=ws://localhost:8080

# Production WebSocket Configurations
PROD_WEBSOCKET_PRIMARY_URL=wss://y-webrtc-signal-backend.fly.dev
PROD_WEBSOCKET_FALLBACK_URL=wss://y-websocket-backup.herokuapp.com

# Connection Strategy
WEBSOCKET_CONNECT_TIMEOUT=10000
WEBSOCKET_RECONNECT_ATTEMPTS=3
WEBSOCKET_RECONNECT_INTERVAL=2000

# Deployment Flags
ENABLE_COLLABORATION=true
ENABLE_WEBSOCKET_FALLBACK=true

# Logging and Debugging
DEBUG_WEBSOCKET=true
LOG_LEVEL=info

# Performance Tuning
MAX_CONCURRENT_CONNECTIONS=100
CONNECTION_TIMEOUT=30000
